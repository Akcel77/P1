    <script>


        let scrollpos = window.scrollY
        const header = document.querySelector("nav")
        const header_height = 900

        const add_class_on_scroll = () => header.classList.add("bg-dark")
        const add_dis_on_scroll = () => header.classList.add("d-none")
        const remove_class_on_scroll = () => header.classList.remove("bg-dark")
        const remove_dis_on_scroll = () => header.classList.remove("d-none")

        window.addEventListener('scroll', function() {
            scrollpos = window.scrollY;

            if (scrollpos >= header_height) { add_class_on_scroll() }
            else { remove_class_on_scroll() }
        })
    </script>



