    <script>


        let scrollpos = window.scrollY
        const header = document.querySelector("nav")
        const header_height = 400

        const add_class_on_scroll = () => header.classList.add("bg-dark")
        const add_dis_on_scroll = () => header.classList.add("d-none")
        const remove_class_on_scroll = () => header.classList.remove("bg-dark")
        const remove_dis_on_scroll = () => header.classList.remove("d-none")

        document.addEventListener("DOMContentLoaded", function() {
            var logo = document.querySelector(".ohana.dark");
            var logoHidden = document.querySelector(".ohana-logo-hidden");

            window.addEventListener("scroll", function() {
                var scrollTop = window.pageYOffset || document.documentElement.scrollTop;

                if (scrollTop > 400) {
                    logo.classList.add("ohana-logo-visible");
                    logoHidden.classList.remove("ohana-logo-hidden");
                } else {
                    logo.classList.remove("ohana-logo-visible");
                    logoHidden.classList.add("ohana-logo-hidden");
                }
            });
        });

        window.addEventListener('scroll', function() {
            scrollpos = window.scrollY;

            if (scrollpos >= header_height) { add_class_on_scroll() }
            else { remove_class_on_scroll() }
        })
    </script>



